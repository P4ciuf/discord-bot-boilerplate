{
  "name": "main-bot-discord",
  "version": "beta-1.0.0",
  "main": "dist/src/main.js",
  "scripts": {
    "test": "bun src/main.ts",
    "start": "npm run build && node dist/src/main.js",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "xcopy src\\assets dist\\src\\assets /E /I /Y",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "delete-commands": "bun run scripts/deleteCommands.ts",
    "register-commands": "bun run scripts/registerCommands.ts"
  },
  "dependencies": {
    "@techtribe-studio/logger": "^0.1.2",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "ms": "^2.1.3",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/ms": "^2.1.0",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
